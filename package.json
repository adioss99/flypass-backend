  "main": "index.js",
  "scripts": {
    "start": "node ./app/index.js",
    "dev": "nodemon ./app/index.js",
    "test": "jest --coverage",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all",
    "db:refresh": "sequelize db:drop && sequelize db:drop && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:test-seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint .",
    "lint:fix": "eslint --fix"
  },
